  "resolutions": {
    "react-dates/lodash": "^4.17.21",
    "react-test-renderer": "^16.13.1"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node --icu-data-dir=node_modules/full-icu"
    }
  },
  "scripts": {
    "audit": "yarn audit --json | node scripts/audit.js",
    "clean": "rm -rf build/*",
    "config": "node scripts/config.js",
    "config-check": "node scripts/config.js --check",
    "dev-frontend": "sharetribe-scripts start",
    "dev-backend": "nodemon apiServer.js",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && yarn run config-check && set NODE_ENV=development && set REACT_APP_DEV_API_SERVER_PORT=3500 && concurrently --kill-others \"yarn run dev-backend\"",
    "dev-mac": "yarn run config-check&&export NODE_ENV=development REACT_APP_DEV_API_SERVER_PORT=3500&&concurrently --kill-others \"yarn run dev-frontend\" \"yarn run dev-backend\"",
    "build": "yarn build-server",
    "build-web": "sharetribe-scripts build",
    "build-server": "sharetribe-scripts build-server",
    "format": "prettier --write '**/*.{js,css}'",
    "format-ci": "prettier --list-different '**/*.{js,css}'",
    "format-docs": "prettier --write '**/*.md'",
    "test": "NODE_ICU_DATA=node_modules/full-icu sharetribe-scripts test",
    "test-ci": "yarn run test-server --runInBand && sharetribe-scripts test --runInBand",
    "eject": "sharetribe-scripts eject",
    "start": "node apiServer.js",
    "dev-server": "set NODE_ENV=development PORT=4000 REACT_APP_CANONICAL_ROOT_URL=http://localhost:4000&&yarn run build&&nodemon --watch server server/index.js",
    "dev-server-mac": "export NODE_ENV=development PORT=4000 REACT_APP_CANONICAL_ROOT_URL=http://localhost:4000&&yarn run build&&nodemon --watch server server/index.js",
    "test-server": "jest ./**/*.test.js",
    "heroku-postbuild": "yarn run build",
    "translate": "node scripts/translations.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "<= 16.18.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  }
}
